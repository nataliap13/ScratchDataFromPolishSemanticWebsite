@model Test.Models.InputWords

<div class="container">
	Witaj na stronie wyszukującej najkrótsze ścieżki pomiędzy dwoma wyrazami korzystającej z <a href="http://nlp.pwr.wroc.pl/wordnet/msr/">funkcji podobieństwa znaczeniowego jednostek leksykalnych</a>.<br />
	Podaj dwa wyrazy lub wyrażenia i sprawdź jak działa.
	<div class="row">
		<form asp-controller="WWW" asp-action="Index" method="post">
			<div class="form-group">
				<label asp-for="BeginWord"></label>
				<input class="form-control" asp-for="BeginWord" value="człowiek">
			</div>
			<div class="form-group">
				<label asp-for="EndWord"></label>
				<input class="form-control" asp-for="EndWord" value="pies">
			</div>
			<button type="submit" class="btn btn-primary">Szukaj</button>
		</form>
	</div>
</div>

